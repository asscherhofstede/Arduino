<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relativeLayout1">
        <TextView
            android:text="@string/timer_state_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewTimerState"
            android:layout_alignParentLeft="true"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/timer_state_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewTimerStateValue"
            android:layout_alignParentRight="true"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@android:color/holo_orange_dark" />
        <TextView
            android:text="@string/arduino_server_text"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewServer"
            android:layout_below="@id/textViewTimerState"
            android:layout_alignParentLeft="true" />
        <TextView
            android:text="@string/arduino_server_connect"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewServerConnect"
            android:layout_below="@id/textViewTimerStateValue"
            android:layout_alignParentRight="true"
            android:textColor="@android:color/holo_red_dark" />
        <TextView
            android:text="@string/ip_address_text"
            android:layout_width="150dp"
            android:layout_height="20dp"
            android:id="@+id/textViewIPAddress"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewServer" />
        <TextView
            android:text="@string/ip_port_text"
            android:layout_width="200dp"
            android:layout_height="20dp"
            android:id="@+id/textViewIPPort"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewServerConnect" />
        <EditText
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/editTextIPAddress"
            android:text="@string/ip_address_value"
            android:lines="1"
            android:minLines="1"
            android:maxLines="1"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:layout_below="@id/textViewIPAddress"
            android:layout_alignParentLeft="true" />
        <EditText
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:id="@+id/editTextIPPort"
            android:text="@string/ip_port_value"
            android:lines="1"
            android:minLines="1"
            android:maxLines="1"
            android:inputType="numberDecimal"
            android:digits="0123456789"
            android:layout_below="@id/textViewIPPort"
            android:layout_alignParentRight="true" />
        <Button
            android:text="@string/button_connect_text"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/editTextIPAddress"
            android:id="@+id/buttonConnect"
            android:layout_alignParentLeft="true"
            android:backgroundTint="#add8e6ff" />
    <!--
      Hier komen alle stopcontact buttons
      -->
    <!--Stopcontact 1-->
        <Button
            android:text="@string/button_pin_state_text1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/buttonConnect"
            android:id="@+id/buttonChangePinState1"
            android:backgroundTint="#add8e6ff" />
        <TextView
            android:text="@string/pin_state_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinState1"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/buttonChangePinState1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/change_pin_state_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinStateValue1"
            android:layout_alignParentRight="true"
            android:layout_below="@id/buttonChangePinState1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensor1"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewChangePinState1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensorValue1"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewChangePinStateValue1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebug1"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewSensor1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebugValue1"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewSensorValue1"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    <!--Stopcontact 2-->
        <Button
            android:text="@string/button_pin_state_text2"
            android:id="@+id/buttonChangePinState2"
            android:backgroundTint="#add8e6ff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/textViewDebugValue1" />
        <TextView
            android:text="@string/pin_state_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinState2"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/buttonChangePinState2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/change_pin_state_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinStateValue2"
            android:layout_alignParentRight="true"
            android:layout_below="@id/buttonChangePinState2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensor2"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewChangePinState2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensorValue2"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewChangePinStateValue2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebug2"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewSensor2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebugValue2"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewSensorValue2"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    <!--Stopcontact 3-->
        <Button
            android:text="@string/button_pin_state_text3"
            android:id="@+id/buttonChangePinState3"
            android:backgroundTint="#add8e6ff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/textViewDebugValue2" />
        <TextView
            android:text="@string/pin_state_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinState3"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/buttonChangePinState3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/change_pin_state_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewChangePinStateValue3"
            android:layout_alignParentRight="true"
            android:layout_below="@id/buttonChangePinState3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensor3"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewChangePinState3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/sensor_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewSensorValue3"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewChangePinStateValue3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_text"
            android:layout_width="150dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebug3"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/textViewSensor3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        <TextView
            android:text="@string/debug_value"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:id="@+id/textViewDebugValue3"
            android:layout_alignParentRight="true"
            android:layout_below="@id/textViewSensorValue3"
            android:textAppearance="?android:attr/textAppearanceMedium" />
    <!--Refresh instellen-->
        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/refreshTextInput"
            android:text="@string/refresh_Text_Input"
            android:lines="1"
            android:minLines="1"
            android:maxLines="1"
            android:inputType="numberDecimal"
            android:digits="0123456789."
            android:layout_below="@id/textViewDebugValue3" />
        <Button
            android:text="@string/refresh_Button"
            android:id="@+id/refreshButton"
            android:backgroundTint="#add8e6ff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/refreshTextInput" />
    </RelativeLayout>
</LinearLayout>